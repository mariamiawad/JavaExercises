
public class PatternD {

	public PatternD() {
		// TODO Auto-generated constructor stub
	}
	public static void main(String[] args) {


		for (int row = 0; row < 6; row++) {
			System.out.print("  ");
			
			for (int column = 1; column< 7; column++) {
				System.out.print(column);
			}
		

			System.out.println();
		}

	
	}

}
