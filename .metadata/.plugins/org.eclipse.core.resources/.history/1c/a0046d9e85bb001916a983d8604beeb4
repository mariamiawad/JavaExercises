
public class PatternD {

	public PatternD() {
		// TODO Auto-generated constructor stub
	}
	public static void main(String[] args) {


		for (int row = 6; row > 0; row--) {
		
			for (int i =1; i <row; i++) {
				System.out.print(i);
				
			}
			for (int column = 1; column<row; column++) {
				System.out.print(" ");
			}
			
			
		

			System.out.println();
		}

	
	}

}
