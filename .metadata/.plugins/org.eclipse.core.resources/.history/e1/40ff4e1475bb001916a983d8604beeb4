
public class PatternC {

	public PatternC() {
		// TODO Auto-generated constructor stub
	}
public static void main(String[] args) {


	for (int row = 1; row <7; row++) {
		for (int column = 6; column > row; column--) {
			System.out.print(" ");
		}
		for (int i = 1; i <=row ; i++) {
			
		}
	}


}
}
