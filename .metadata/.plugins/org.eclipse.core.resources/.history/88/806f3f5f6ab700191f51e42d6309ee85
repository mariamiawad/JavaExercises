class Test
{
    // METHOD SIGNATURE BEGINS, THIS METHOD IS REQUIRED
    public int generalizedGCD(int num, int[] arr)
    {
      int max = 0;
      for (int i = 1, j = 2;i< arr.length-1 ;i++, j++){
          
          max = getGCD(arr[i], arr[i-1]);
      } 
      return max;
    }

    private int getGCD(int a, int b) {
        if(a%b== 0){
            if(a<b){
                return a;
            }
            else if(a>b){
                return b;
            }
        }
        else {
        	return getGCD(a, b);
        }
		return 1;
    }

	
        
    
    // METHOD SIGNATURE ENDS
}