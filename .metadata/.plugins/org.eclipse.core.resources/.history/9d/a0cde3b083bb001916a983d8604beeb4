
public class PatternD {

	public PatternD() {
		// TODO Auto-generated constructor stub
	}
	public static void main(String[] args) {


		for (int row = 0; row < 6; row++) {
			for (int i = 1; i <7; i++) {
				System.out.print(i);
			}
			
			for (int column = 1; column< 7; column++) {
				System.out.print(" ");
			}
		

			System.out.println();
		}

	
	}

}
