
public class PatternC {

	public PatternC() {
		// TODO Auto-generated constructor stub
	}
public static void main(String[] args) {
	for (int row = 1; row < 7; row++) {
		for (int column = 6, j=1; column >=row; column--) {
			System.out.print(" " +j);
			j++;
		}
//		for (int i = 0; i < args.length; i++) {
//			
//		}
		System.out.println();
	}
}
}
