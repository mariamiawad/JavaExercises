
public class PatternD {

	public PatternD() {
		// TODO Auto-generated constructor stub
	}
	public static void main(String[] args) {


		for (int row = 6; row > 0; row--) {
			for (int column = 0; column<row; column++) {
				System.out.print(" " +column);
			}
			for (int i = 1; i < row; i++) {
				System.out.print(" " +row);
				
			}
//			
			
			
		

			System.out.println();
		}

	
	}

}
