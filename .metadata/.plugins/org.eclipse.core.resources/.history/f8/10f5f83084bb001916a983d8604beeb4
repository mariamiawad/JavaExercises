
public class PatternD {

	public PatternD() {
		// TODO Auto-generated constructor stub
	}
	public static void main(String[] args) {


		for (int row = 7; row > 0; row--) {
			
			
			for (int column = 1; column<row; column++) {
				System.out.print(" " + row);
			}
			
		

			System.out.println();
		}

	
	}

}
